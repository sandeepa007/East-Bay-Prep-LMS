<div id="quiz_overview" class="pt-20">

     <div class="w-full">
        <div class="col-md-12">
            <select id='coursecategorylist' class='form-control mb-10'>
                <option value="">{{#str}}selectacourse, theme_remui{{/str}}</option>
                {{#quizcourse}}
                    <option data-id="{{id}}">{{{name}}}</option>
                {{/quizcourse}}
            </select>
        </div>
        <div class="col-md-12">
            <div id="analysis-chart-area">
                <div class="analysis-chart-wrapper">
                    <canvas id="analysischart"></canvas>
                </div>
            </div>
        </div>

        <div id = "course-stats" class="row px-15">
            <div class="col-md-4 col-sm-12 py-20 d-flex align-items-center flex-column">
                <div id="highestgradelabel" class="counter-label font-size-20 font-weight-600">{{#str}}highestgrade, theme_remui{{/str}}</div>
                <div id="highestgrade" class="counter-label display-4 green-500">0</div>
                <div id="highestactivity" class="counter-label text-center"></div>
            </div>
            <div class="col-md-4 col-sm-12 py-20 d-flex align-items-center flex-column">
                <div id="lowestgradelabel" class="counter-label font-size-20 font-weight-600">{{#str}}lowestgrade, theme_remui{{/str}}</div>
                <div id="lowestgrade" class="counter-label display-4 red-500">0</div>
                <div id="lowestactivity" class="counter-label  text-center"></div>
            </div>
            <div class="col-md-4 col-sm-12 py-20 d-flex align-items-center flex-column">
                <div id="averagegradelabel" class="counter-label font-size-20 font-weight-600">{{#str}}averagegrade, theme_remui{{/str}}</div>
                <div id="averagegrade" class="counter-label display-4 purple-500">0</div>
                <div id="allactivities" class="counter-label text-center">{{#str}}allActivities, theme_remui{{/str}}</div>
            </div>
        </div>
    </div>

</div>

{{#js}}
    require(['block_remuiblck/courseanalytics'], function(){});
{{/js}}
